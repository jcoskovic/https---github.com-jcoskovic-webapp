{
  "name": "abbrevio",
  "version": "1.0.0",
  "description": "Web aplikacija za upravljanje skraÄ‡enicama u organizaciji",
  "scripts": {
    "preinstall": "rm -f package-lock.json",
    "install": "npm install --legacy-peer-deps",
    "start": "cd frontend && serve -s dist/frontend -l 4200",
    "build": "cd frontend && ng build --configuration production",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && php artisan serve",
    "ml-service": "cd ml-service && python app.py",
    "setup:frontend": "cd frontend && npm install",
    "setup:backend": "cd backend && composer install && cp .env.example .env && php artisan key:generate",
    "setup:ml": "cd ml-service && pip install -r requirements.txt",
    "setup": "npm run setup:frontend && npm run setup:backend && npm run setup:ml",
    "migrate": "cd backend && php artisan migrate --seed",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && php artisan test",
    "lint": "cd frontend && npm run lint"
  },
  "keywords": [
    "abbreviations",
    "angular",
    "laravel",
    "machine-learning",
    "api"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-organization/abbrevio.git"
  },
  "dependencies": {
    "@angular/animations": "18.2.12",
    "@angular/cdk": "18.2.13",
    "@angular/common": "18.2.12",
    "@angular/compiler": "18.2.12",
    "@angular/core": "18.2.12",
    "@angular/forms": "18.2.12",
    "@angular/material": "18.2.13",
    "@angular/platform-browser": "18.2.12",
    "@angular/platform-browser-dynamic": "18.2.12",
    "@angular/router": "18.2.12",
    "rxjs": "~7.8.0",
    "serve": "^14.2.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "18.3.1",
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  }
}
